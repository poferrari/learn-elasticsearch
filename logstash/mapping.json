{
    "index_patterns": [
        "*"
    ],
    "settings": {
        "number_of_shards": 1,
        "analysis": {
            "filter": {
                "asciifolding": {
                    "type": "asciifolding",
                    "preserve_original": true
                }
            },
            "analyzer": {
                "custom_analyzer": {
                    "tokenizer": "standard",
                    "filter": [
                        "lowercase",
                        "asciifolding"
                    ]
                }
            }
        }
    },
    "mappings": {
        "properties": {
            "Name": {
                "type": "text",
                "analyzer": "custom_analyzer"
            },          
            "AgencyName": {
                "type": "text",
                "analyzer": "standard"
            },
            "CPF": {
                "type": "keyword",
                "ignore_above": 50
            },  
            "FunctionDescription": {
                "type": "keyword"                
            },
            "FunctionLevel": {
                "type": "keyword"                
            },
            "StartDateExercise": {
                "type": "date"                
            },
            "EndDateExercise": {
                "type": "date"            
            },
            "EndDateWaitingPeriod": {
                "type": "date"                
            }
        }
    }
}